    <webLinks>
        <fullName>Track_Consignment</fullName>
        <availability>online</availability>
        <description>open consignment search page</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Track Consignment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}
if(&apos;{!Case.UUCN_Number__c}&apos; != &apos;&apos;)
{
	if(sforce.console.isInConsole())
	{
		var openSubtab = function openSubtab(result) {
				var primaryTabId = result.id;
				sforce.console.openSubtab(primaryTabId , &quot;/apex/OL_SearchConsignmentById?lock=1&amp;conId={!Case.UUCN_Number__c}&quot;, true, &apos;Consignment Tracking&apos;, null);
		};
		sforce.console.getEnclosingPrimaryTabId(openSubtab);
	}
	else
	{
		window.open(&quot;/apex/OL_SearchConsignmentById?lock=1&amp;conId={!Case.UUCN_Number__c}&quot;,&quot;_blank&quot;);
	}
}
else
{
	alert(&quot;UUCN Number is blank.&quot;);
}</url>
    </webLinks>
