    <fields>
        <fullName>Am_I_the_record_Owners_Manager__c</fullName>
        <description>Used to calculate whether the logged in user is the record Owners Manger or not.</description>
        <externalId>false</externalId>
        <formula>IF( Owner:User.ManagerId = $User.Id , True , False)</formula>
        <label>Am I the record Owners Manager?</label>
