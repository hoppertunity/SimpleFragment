    <webLinks>
        <fullName>Create_Booking</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Create Booking</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)}
    if ({!Quote__c.Quote_is_Valid__c}){
	if(sforce.console.isInConsole())
	{
		var openSubtab = function openSubtab(result) {
			var primaryTabId = result.id;
			sforce.console.openSubtab(primaryTabId , &quot;/apex/OL_BookingEditNew?qId={!Quote__c.Id}\&amp;retURL=%2F{!Quote__c.Id}&quot;, true, &apos;New Booking&apos;, null);
		};
		sforce.console.getEnclosingPrimaryTabId(openSubtab);
	}
	else
	{
		window.open(&quot;/apex/OL_BookingEditNew?qId={!Quote__c.Id}\&amp;retURL=%2F{!Quote__c.Id}&quot;,&quot;_blank&quot;);
	}
} else {
	alert(&quot;Quote is not valid. Create new Quote to be able to create a Booking.&quot;);
}</url>
    </webLinks>
