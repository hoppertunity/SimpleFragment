        <errorDisplayField>Dangerous_Goods__c</errorDisplayField>
        <errorMessage>You cannot save a dangerous goods item to an account which doesn&apos;t ship dangerous goods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delivery_Postcode_or_Town_are_mandatory</fullName>
        <active>false</active>
        <description>Delivery Postcode or Town are requier</description>
        <errorConditionFormula>ISBLANK( Delivery_Postcode__c ) &amp;&amp; ISBLANK( Delivery_Town__c )</errorConditionFormula>
        <errorDisplayField>Delivery_Postcode__c</errorDisplayField>
        <errorMessage>Delivery Postcode or Town should be entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_or_Contact</fullName>
        <active>true</active>
        <description>Email can only be entered if there is no Contact found and vice versa.</description>
        <errorConditionFormula>Email_the_quote__c &amp;&amp;  Email_Only__c = &apos;&apos; &amp;&amp;   Contact__r.Email  = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Email_the_quote__c</errorDisplayField>
        <errorMessage>To email the quote, you have to select a contact with a stored email address, or populate the email field with an email address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Stop_Quote_Prevention_Rule</fullName>
        <active>false</active>
        <description>When the On Stop field is checked, the Account is ON STOP. Only a Stubbins user is able to edit the status of an Account which is ON STOP. No Quote can be saved against this account without permissions from Stubbins.</description>
        <errorConditionFormula>Account__r.On_Stop__c = TRUE &amp;&amp; !Account__r.Stubbins_Temporary_Permission__c</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>This Account is On Stop. Approval must be given from Stubbins to save a Quote for this Account.</errorMessage>
    </validationRules>
