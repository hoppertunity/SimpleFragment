        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/support/console/32.0/integration.js&quot;)}
var totalWeightStr = &apos;{!Quote__c.Total_Weight__c}&apos;;
totalWeightStr = totalWeightStr.replace(&quot;,&quot;, &quot;&quot;);
var totalWeight = parseInt(totalWeightStr);
if(totalWeight &lt; 1){
	alert(&quot;Please enter at least one Quote line to be able to request price.&quot;);
} else {
	if({!Quote__c.Quote_is_Valid__c})
	{
		if(sforce.console.isInConsole())
		{
			var openSubtab = function openSubtab(result) {
					var primaryTabId = result.id;
					sforce.console.openSubtab(primaryTabId , &quot;/apex/OL_GetQuotePricingDetails?id={!Quote__c.Id}&quot;, true, &apos;Quote Pricing&apos;, null);
			};
			sforce.console.getEnclosingPrimaryTabId(openSubtab);
		}
		else
		{
			window.location=&quot;/apex/OL_GetQuotePricingDetails?id={!Quote__c.Id}&quot;;
		}
	}
	else
	{
		alert(&quot;This Quote has expired and cannot be priced.&quot;);
	}
}</url>
