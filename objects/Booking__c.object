        <errorConditionFormula>Account__r.Authorization_for_booking_required__c &amp;&amp; NOT( Order_Contact__r.Authorized_for_bookings__c )</errorConditionFormula>
        <errorDisplayField>Order_Contact__c</errorDisplayField>
        <errorMessage>This contact is not authorised to make bookings</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commercial_Invoice_Required</fullName>
        <active>true</active>
        <description>The validation rule for this field would be that if the County/Region field = Channel Islands then the Commerical Invoice Field has to be set to Yes</description>
        <errorConditionFormula>(LOWER(Coll_County__c) == &apos;channel islands&apos; || LOWER(Del_County__c) == &apos;channel islands&apos;) &amp;&amp;  Commercial_Invoice_Required__c == false &amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)</errorConditionFormula>
        <errorMessage>When Collection or Delivery County/Region is &apos;Channel Islands&apos;, &apos;Commercial Invoice Required&apos; Field should be set to &apos;Yes&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Commodity_required</fullName>
        <active>true</active>
        <description>Required field missing: Type of Packaging (Commodity)</description>
        <errorConditionFormula>ISBLANK(TEXT(Commodity__c)) &amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)</errorConditionFormula>
        <errorMessage>Required field missing: Type of Packaging (Commodity)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Reference_Required</fullName>
        <active>true</active>
        <errorConditionFormula>Account__r.Customer_Reference_Required__c &amp;&amp;  ISBLANK(Customer_Reference__c) &amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)</errorConditionFormula>
        <errorMessage>This Account requires a Reference. Please enter one</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Dangerous_Goods</fullName>
        <active>false</active>
        <description>You cannot save a dangerous goods item to an account which doesn&apos;t ship dangerous goods.</description>
        <errorConditionFormula>ISPICKVAL(Account__r.Dangerous_Goods__c,&apos;No&apos;) &amp;&amp; Dangerous_Goods__c</errorConditionFormula>
        <errorDisplayField>Dangerous_Goods__c</errorDisplayField>
        <errorMessage>You cannot save a dangerous goods item to an account which doesn&apos;t ship dangerous goods.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_allow_Status_None</fullName>
        <active>true</active>
        <description>Validation rule to stop users selecting a booking status of None</description>
        <errorConditionFormula>ISBLANK(TEXT(Status__c)) &amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please select value for Booking Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_address_details_required</fullName>
        <active>true</active>
        <description>The Booking record can only be saved with the &apos;email the booking reference?&apos; field checked if there is a valid Email address for the contact</description>
        <errorConditionFormula>Email_the_booking_reference__c  &amp;&amp;  OC_Email_Address__c  = &apos;&apos; &amp;&amp;   Order_Contact__r.Email  = &apos;&apos;</errorConditionFormula>
        <errorDisplayField>Email_the_booking_reference__c</errorDisplayField>
        <errorMessage>To email the booking, you have to select a contact with a stored email address, or populate the email field with an email address</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>On_Stop_Booking_Prevention_Rule</fullName>
        <active>false</active>
        <description>When the On Stop field is checked, the Account is ON STOP. Only a Stubbins user is able to edit the status of an Account which is ON STOP. No bookings can be saved against this account without permissions from Stubbins.</description>
        <errorConditionFormula>Account__r.On_Stop__c = TRUE &amp;&amp;  !Account__r.Stubbins_Temporary_Permission__c</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>This Account is On Stop. Approval must be given from Stubbins to save a Booking for this Account.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Weight_must_be_less_than_99999kg</fullName>
        <active>true</active>
        <description>This is required because Universe can only store up to 99999. EDW can store up to 169000</description>
        <errorConditionFormula>Total_Weight__c  &gt; 99999</errorConditionFormula>
        <errorMessage>The &apos;Total Weight&apos; for a Booking must be less than 99999.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unconfirmed_Bookings</fullName>
        <active>false</active>
        <description>An agent can confirm manually a Booking only when it&apos;s Unconfirmed</description>
        <errorConditionFormula>ISPICKVAL( Status__c ,&apos;Confirmed&apos;) &amp;&amp;  NOT(TEXT(PRIORVALUE(Status__c)) = &apos;Unconfirmed&apos;) &amp;&amp;   NOT(TEXT(PRIORVALUE(Status__c)) = &apos;Not Collected&apos;) &amp;&amp; NOT(TEXT(PRIORVALUE(Status__c)) = &apos;Rejected to Customer Services&apos;) &amp;&amp; NOT(TEXT(PRIORVALUE(Status__c)) = &apos;&apos;) &amp;&amp;  ISCHANGED(Status__c)
&amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Only bookings with a current Status of Unconfirmed, Rejected to Customer Services or Not Collected can be changed to a &apos;Confirmed&apos; status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Phone_Numbers_CC_Phone_Number</fullName>
        <active>true</active>
        <description>Ensures that bookings made in the system are created with an NVM compatible phone number</description>
        <errorConditionFormula>NOT(REGEX( CC_Phone_Number__c , &quot;[0-9]{7,15}&quot;))
&amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)
&amp;&amp; NOT(CreatedBy.Profile.Name = &apos;Integration User&apos;)   
&amp;&amp; NOT(ISBLANK(CC_Phone_Number__c))</errorConditionFormula>
        <errorMessage>Please enter a valid phone number (7-15 digits, no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Phone_Numbers_DC_Phone_Number</fullName>
        <active>true</active>
        <description>Ensures that bookings made in the system are created with an NVM compatible phone number</description>
        <errorConditionFormula>NOT(REGEX( DC_Phone_Number__c , &quot;[0-9]{7,15}&quot;))
&amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)
&amp;&amp; NOT(CreatedBy.Profile.Name = &apos;Integration User&apos;)   
&amp;&amp; NOT(ISBLANK(DC_Phone_Number__c))</errorConditionFormula>
        <errorMessage>Please enter a valid phone number (7-15 digits, no spaces)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Phone_Numbers_OC_Phone_Number</fullName>
        <active>true</active>
        <description>Ensures that bookings made in the system are created with an NVM compatible phone number</description>
        <errorConditionFormula>NOT(REGEX( OC_Phone_Number__c , &quot;[0-9]{7,15}&quot;))
&amp;&amp; NOT($Profile.Name = &apos;System Administrator&apos;)
&amp;&amp; NOT($Profile.Name = &apos;Integration User&apos;)
&amp;&amp; NOT (CreatedBy.Profile.Name = &apos;Integration User&apos;)  
&amp;&amp; NOT(ISBLANK(OC_Phone_Number__c))</errorConditionFormula>
        <errorMessage>Please enter a valid phone number (7-15 digits, no spaces)</errorMessage>
    </validationRules>
