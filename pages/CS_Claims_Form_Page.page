                    <apex:pageBlockButtons location="bottom" dir="RTL">
                    		<apex:commandButton action="{!save_m}" value="Click to Submit"/>
                         <apex:commandButton action="{!quickSave}" value="Save & Submit Later" rendered="true" />                                                                                                             
