        <apex:includeScript value="{!URLFOR($Resource.jquery1110)}" id="jquery"/>
        <title>TNT UK Claim Form</title>
        
        <style>
            body{
                font-family:Arial;
                font-size: 11px;
            }
            h1{
                font-size:20px;
                color:#FF6B07;
            }
